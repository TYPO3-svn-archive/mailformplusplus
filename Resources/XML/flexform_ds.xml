		</meta>
<sheets>
	<sDEF>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:mailformplusplus/Resources/Language/locallang_tca.xml:mailformplusplus.pi_flexform.sheet_general</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
			<template_file>
				<TCEforms>
					<label>LLL:EXT:mailformplusplus/Resources/Language/locallang_tca.xml:mailformplusplus.pi_flexform.template_file</label>
					<config>
						<type>group</type>
						<internal_type>file</internal_type>
						<allowed>tmpl,html,htm</allowed>
						<max_size>500</max_size>
						<minitems>0</minitems>
						<maxitems>1</maxitems>
						<size>1</size>
						<selectedListStyle>Width:180px</selectedListStyle>
					</config>
				</TCEforms>
			</template_file>
			<lang_file>
				<TCEforms>
					<label>LLL:EXT:mailformplusplus/Resources/Language/locallang_tca.xml:mailformplusplus.pi_flexform.lang_file</label>
					<config>
						<type>group</type>
						<internal_type>file</internal_type>
						<allowed>php,xml</allowed>
						<max_size>500</max_size>
						<minitems>0</minitems>
						<maxitems>1</maxitems>
						<size>1</size>
						<selectedListStyle>Width:180px</selectedListStyle>
					</config>
				</TCEforms>
			</lang_file>
			<predefined>
				<TCEforms>
					<label>LLL:EXT:mailformplusplus/Resources/Language/locallang_tca.xml:mailformplusplus.pi_flexform.template_predefined</label>
					<config>
					    <type>select</type>
						<itemsProcFunc>tx_dynaflex_mailformplusplus->addFields_predefined</itemsProcFunc>
					    <maxitems>1</maxitems>
					    <size>1</size>
					    <multiple>0</multiple>
					    <selectedListStyle>width:150px</selectedListStyle>
					    <itemListStyle>width:150px</itemListStyle>
					</config>
				</TCEforms>
			</predefined>
			<controller>
				<TCEforms>
					<label>LLL:EXT:mailformplusplus/Resources/Language/locallang_tca.xml:mailformplusplus.pi_flexform.template_controller</label>
					<config>
					    <type>select</type>
						<itemsProcFunc>tx_dynaflex_mailformplusplus->addFields_controller</itemsProcFunc>
					    <maxitems>1</maxitems>
					    <size>1</size>
					    <multiple>0</multiple>
					    <selectedListStyle>width:150px</selectedListStyle>
					    <itemListStyle>width:150px</itemListStyle>
					</config>
				</TCEforms>
			</controller>
		</el>
	</ROOT>
